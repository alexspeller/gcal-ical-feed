{
  "name": "ical-feed",
  "type": "module",
  "main": "dist/index.js",
  "packageManager": "yarn@4.12.0",
  "scripts": {
    "format:fix": "biome check --error-on-warnings --write .",
    "check": "biome check --error-on-warnings .",
    "typecheck": "tsc --noEmit",
    "dev": "node --import=ts-node/esm --watch src/index.ts",
    "start": "node --import=ts-node/esm src/index.ts",
    "auth": "node --import=ts-node/esm src/scripts/get-token.ts"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.13",
    "@types/node": "^25.1.0",
    "typescript": "^5.9.3"
  },
  "dependencies": {
    "@hono/node-server": "^1.19.9",
    "googleapis": "^171.1.0",
    "hono": "^4.11.7",
    "ical-generator": "^10.0.0",
    "ts-node": "^10.9.2"
  }
}
